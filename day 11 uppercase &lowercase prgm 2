#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main() {
    char str[200]; 
    printf("Enter a string: ");
    fgets(str, sizeof(str), stdin); 
    for (int i = 0; str[i], != '\0'; ++i) 
    {
    if (str[i] >= 'a' && str[i] <= 'z')
    {
        str[i] = str[i] - 32;
    }
}
printf("uppercase string: %s\n", str);
    return 0;
}
